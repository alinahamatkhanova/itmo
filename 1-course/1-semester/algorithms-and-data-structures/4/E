#include <iostream>
using namespace std;

void siftDown(int a[], int i, int heapSize) {
    while (2 * i + 1 < heapSize) {
        int left = 2 * i + 1;
        int right = 2 * i + 2;
        int j = left;
        if (right < heapSize && a[right] > a[left]) {
            j = right;
        }
        if (a[i] >= a[j]) {
            break;
        }
        swap(a[i], a[j]); i = j;
    }
}
void heapSort(int a[], int n) {
    for (int i = n / 2 - 1; i >= 0; i--) {
        siftDown(a, i, n);
    }
    for (int i = n - 1; i > 0; i--) {
        swap(a[0], a[i]); siftDown(a, 0, i);
    }
}
int main() {
    int n;
    cin >> n;
    
    int sokrow[n];
    for (int i = 0; i < n; i++) {
        cin >> sokrow[i];
    }
    heapSort(sokrow, n);
    for (int i = 0; i < n; i++) {
        cout << sokrow[i] << " ";
    }
    cout << endl;
    return 0;
}
